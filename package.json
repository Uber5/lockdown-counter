{
  "name": "lockdown-counter",
  "private": true,
  "description": "Provide a countdown until the South African lockdown related to Covid-19 supposedly ends, and other information",
  "version": "0.1.0",
  "author": "chris@uber5.com",
  "dependencies": {
    "date-fns": "^2.11.1",
    "gatsby": "^2.19.45",
    "gatsby-image": "^2.2.44",
    "gatsby-plugin-google-analytics": "^2.2.2",
    "gatsby-plugin-manifest": "^2.3.3",
    "gatsby-plugin-offline": "^3.0.41",
    "gatsby-plugin-react-helmet": "^3.1.24",
    "gatsby-plugin-sharp": "^2.5.3",
    "gatsby-source-filesystem": "^2.1.56",
    "gatsby-transformer-sharp": "^2.4.3",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
